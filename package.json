{
  "name": "test-runner-register",
  "version": "0.0.0",
  "description": "In-source testing with the Node.js Test Runner",
  "files": [
    "auto.cjs",
    "cjs.js",
    "esm.cjs",
    "globals.d.ts",
    "auto.mjs",
    "swc.mjs",
    "ts-node.mjs"
  ],
  "exports": {
    ".": {
      "require": "./auto.cjs",
      "import": "./auto.cjs"
    },
    "./cjs": {
      "require": "./cjs.js"
    },
    "./esm": {
      "require": "./esm.cjs",
      "import": "./esm.cjs"
    },
    "./globals": {
      "types": "./globals.d.ts"
    },
    "./import": {
      "import": "./auto.mjs"
    },
    "./import/swc": {
      "import": "./swc.mjs"
    },
    "./import/ts-node": {
      "import": "./ts-node.mjs"
    }
  },
  "optionalDependencies": {
    "@swc-node/register": "^1.6.8",
    "ts-node": "^10.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tai-kun/test-runner-register"
  },
  "author": {
    "name": "tai-kun",
    "url": "https://github.com/tai-kun"
  },
  "scripts": {
    "ci": "bash scripts/ci.sh",
    "test": "bash scripts/test.sh",
    "update": "bash scripts/update.sh"
  },
  "volta": {
    "node": "18.17.1"
  }
}
