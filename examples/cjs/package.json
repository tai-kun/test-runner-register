{
  "name": "test-runner-register-cjs",
  "type": "commonjs",
  "engines": {
    "node": "*"
  },
  "scripts": {
    "test:static": "tsc --noEmit",
    "test:unit:swc": "node -r test-runner-register -r @swc-node/register ./main.ts",
    "test:unit:esbuild": "node -r test-runner-register -r esbuild-register ./main.ts",
    "test:unit:ts-node": "node -r test-runner-register -r ts-node/register ./main.ts",
    "test:unit:tsx": "tsx -r test-runner-register ./main.ts"
  },
  "devDependencies": {
    "@swc-node/register": "^1.6.8",
    "@types/node": "^18.17.13",
    "esbuild-register": "^3.5.0",
    "test-runner-register": "file:../..",
    "ts-node": "^10.9.1",
    "tsx": "^4.5.0",
    "typescript": "^5.2.2"
  }
}
