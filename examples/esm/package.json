{
  "name": "test-runner-register-esm",
  "type": "module",
  "scripts": {
    "test:static": "tsc --noEmit",
    "test:unit:swc": "node -r test-runner-register --loader @swc-node/register/esm main.ts",
    "test:unit:esbuild": "node -r test-runner-register --loader esbuild-register/loader -r esbuild-register main.ts",
    "test:unit:ts-node": "node -r test-runner-register --loader ts-node/esm main.ts"
  },
  "devDependencies": {
    "@swc-node/register": "^1.6.8",
    "@types/node": "^18.17.13",
    "esbuild-register": "^3.5.0",
    "test-runner-register": "file:../..",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
